<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        <link rel="stylesheet" href="{{ path("index") }}css/bootstrap.min.css"/>
        <link rel="stylesheet" href="{{ path("index") }}css/jquery-ui.min.css"/>
        <script src="{{ path("index") }}js/jquery-3.6.0.min.js"></script>
        <script src="{{ path("index") }}js/bootstrap.bundle.min.js"></script>
        <script src="{{ path("index") }}js/jquery-ui.min.js"></script>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>

    <body style="max-width:100%;" class="bg-dark">
    {%if app.user is not null%}
        <a href="{{path('app_logout')}}">
            <img src="{{asset('logout-svgrepo-com.svg')}}" style="width: 40px; filter: invert(100%); display: block; float: right; margin-right: 20px; margin-top: 5px">
        </a>
        <a href="{{path('app_pdf_creator')}}">
            <img src="{{asset('printer.svg')}}" style="width: 40px; filter: invert(100%); display: block; float: right; margin-right: 20px; margin-top: 5px">
        </a>
    {%endif%}
        {% block body %}

        {% endblock %}
    </body>
</html>
